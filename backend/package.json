{
  "name": "godfather-backend",
  "version": "1.0.0",
  "description": "Backend pour l'application MY GODFATHER de mentorat",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "migrate": "node src/migrations/migrate.js",
    "migrate:fresh": "cross-env FRESH=true node src/migrations/migrate.js",
    "restore": "node src/migrations/restore.js",
    "restore:backup": "node src/migrations/restore.js",
    "seed": "npm run migrate && node src/migrations/seed.js",
    "seed:fresh": "npm run migrate:fresh && node src/migrations/seed.js",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.js\"",
    "pm2:start": "pm2 start src/index.js --name godfather-backend",
    "pm2:stop": "pm2 stop godfather-backend",
    "pm2:restart": "pm2 restart godfather-backend",
    "pm2:logs": "pm2 logs godfather-backend"
  },
  "dependencies": {
    "@azure/msal-node": "^2.6.4",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-stringify": "^6.5.2",
    "dotenv": "^16.0.3",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "http-errors": "^2.0.0",
    "isomorphic-fetch": "^3.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.1.0",
    "mongoose-paginate-v2": "^1.7.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "pdfkit": "^0.13.0",
    "redis": "^4.6.13",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^2.0.22",
    "pm2": "^5.4.3",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
